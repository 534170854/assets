<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_res_asset" model="ir.sequence">
            <field name="name">Res Asset</field>
            <field name="code">res.asset</field>
            <field name="prefix">asset%(year)s</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False" />
        </record>

        <record id="seq_asset_picking" model="ir.sequence">
            <field name="name">Res Picking</field>
            <field name="code">asset.picking.order</field>
            <field name="prefix">AP%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
        </record>

        <record id="seq_asset_apply_order" model="ir.sequence">
            <field name="name">Asset Apply</field>
            <field name="code">asset.apply.order</field>
            <field name="prefix">APPLY%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
        </record>

        <record id="seq_asset_inventory" model="ir.sequence">
            <field name="name">Res Inventory</field>
            <field name="code">asset.inventory.order</field>
            <field name="prefix">RI%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
        </record>
    </data>
</odoo>